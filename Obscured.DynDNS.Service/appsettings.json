{
  "Domains": [
    {
      "ClientId": "",
      "ClientSecret": "",
      "SubscriptionId": "",
      "TenantId": "",
      "Region": "",
      "ResourceGroup": "",
      "ZoneName": "",
      "RecordName": ""
    }
  ],
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.EventLog", "Serilog.Sinks.File" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Verbose"
        }
      },
      {
        "Name": "EventLog",
        "Args": {
          "manageEventSource": true,
          "restrictedToMinimumLevel": "Error",
          "source": "Obscured.Azure.DynDNS"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\ProgramData\\Obscured.Azure.DynDNS\\debug.production-.log",
          "restrictedToMinimumLevel": "Verbose",
          "retainedFileCountLimit": 7,
          "rollingInterval": "Day",
          "shared": true
        }
      }
    ]
  }
}